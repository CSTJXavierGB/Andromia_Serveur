meta {
  name: [200] Retrieve all listings by explorer
  type: http
  seq: 2
}

get {
  url: {{base_url}}/explorers/:uuidExplorer/listings?type=selling&embed=ally
  body: none
  auth: bearer
}

params:query {
  type: selling
  embed: ally
}

params:path {
  uuidExplorer: {{explorerUUID}}
}

auth:bearer {
  token: {{explorerAccessToken}}
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.data: isArray
}
