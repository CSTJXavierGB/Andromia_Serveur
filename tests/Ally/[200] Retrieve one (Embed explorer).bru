meta {
  name: [200] Retrieve one (Embed explorer)
  type: http
  seq: 1
}

get {
  url: {{base_url}}/allies/:uuid?embed=explorer
  body: none
  auth: inherit
}

params:query {
  embed: explorer
}

params:path {
  uuid: {{allyUUID}}
}

assert {
  res.status: eq 200
  res.body: isJson
  res.body.explorer.vault: isDefined
}
