meta {
  name: [201] - Create an explorer
  type: http
  seq: 1
}

post {
  url: {{base_url}}/explorers
  body: json
  auth: inherit
}

body:json {
  {
    "username":"{{explorerUserName}}",
    "password":"{{explorerPassword}}"
  }
}

assert {
  res.status: eq 201
  res.body: isJson
}

script:post-response {
  const urlInfo = res.body.explorer.href.split("/");
  bru.setEnvVar("explorerUUID", urlInfo.reverse()[0]);
  
  await bru.runRequest("Sessions/[201] - Login");
}
