meta {
  name: [200] Get exploration ally&explorer embed
  type: http
  seq: 3
}

get {
  url: {{base_url}}/explorations/:explorationUUID?embed=ally&embed=explorer
  body: none
  auth: inherit
}

params:query {
  embed: ally
