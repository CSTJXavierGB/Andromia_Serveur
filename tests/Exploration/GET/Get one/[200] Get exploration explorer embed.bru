meta {
  name: [200] Get exploration explorer embed
  type: http
  seq: 4
}

get {
  url: {{base_url}}/explorations/:explorationUUID?embed=explorer
  body: none
  auth: inherit
}

params:query {
  embed: explorer
}

params:path {
  explorationUUID: {{explorationUUID}}
}

assert {
  res.status: eq 200
  res.body: isJson
