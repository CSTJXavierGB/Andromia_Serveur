meta {
  name: [200] Get all exploration from explorer ally&explorer embed
  type: http
  seq: 1
}

get {
  url: {{base_url}}/explorers/:explorerUUID/explorations?embed=ally&embed=explorer
  body: none
  auth: inherit
}

params:query {
  embed: ally
  embed: explorer
}

params:path {
  explorerUUID: {{explorerUUID}}
}

assert {
  res.status: eq 200
  res.body: isArray
  res.body[0].explorer.vault: isDefined
  res.body[0].ally.kernel: isDefined
}
