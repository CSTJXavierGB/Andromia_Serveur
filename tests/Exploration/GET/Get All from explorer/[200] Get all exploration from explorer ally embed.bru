meta {
  name: [200] Get all exploration from explorer ally embed
  type: http
  seq: 3
}

get {
  url: {{base_url}}/explorers/:explorerUUID/explorations?embed=ally
  body: none
  auth: inherit
}

params:query {
  embed: ally
}

params:path {
  explorerUUID: {{explorerUUID}}
}

assert {
  res.status: eq 200
  res.body: isArray
  res.body[0].ally.kernel: isDefined
}
